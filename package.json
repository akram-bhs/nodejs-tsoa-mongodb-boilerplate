{
  "name": "nodejs-tsoa-mongodb-boilerplate",
  "version": "1.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes -c src/config/tsoa/admin.spec.json\" \"nodemon -x tsoa spec-and-routes -c src/config/tsoa/customer.spec.json\"",
    "build": "tsoa spec-and-routes -c src/config/tsoa/admin.spec.json && tsoa spec-and-routes -c src/config/tsoa/customer.spec.json && tsc && copyfiles -u 1 public/docs/**/* dist/public",
    "start": "yarn build && node dist/src/server.js"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1537.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "mongoose": "^8.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "otp-generator": "^4.0.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/hpp": "^0.2.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/method-override": "^0.0.35",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/otp-generator": "^4.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}